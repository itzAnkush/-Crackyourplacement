class Solution {
    public int maxProfit(int[] prices) {
        int buyprice = Integer.MAX_VALUE;
       
        int profit = 0;
        for(int i =0 ; i<prices.length ; i++){
            if(prices[i]<buyprice){
                buyprice = prices[i];
            }
            else if(prices[i]-buyprice>profit){
                profit=prices[i]-buyprice;
            }
        }
        return profit;


    }
}
